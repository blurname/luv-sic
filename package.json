{
  "name": "@blurname/luv-sic",
  "version": "1.0.8",
  "author": "blurname <naughtybao@outlook.com>",
  "private": false,
  "scripts": {
    "mono-test": "bun ./script/monorepo.ts",
    "version-bump": "bun ./script/monorepo.ts version-bump",
    "version-bump-minor": "bun ./script/monorepo.ts version-bump digit=minor",
    "version-bump-major": "bun ./script/monorepo.ts version-bump digit=major",
    "version-bump-ext": "bun ./script/monorepo.ts version-bump-ext",
    "tag-push": "bun ./script/monorepo.ts tag-push",
    "bp": "bun run version-bump && bun run tag-push",
    "tag-push-ext": "bun ./script/monorepo.ts tag-push",
    "release": "npm run version-bump && npm run tag-push",
    "build": "npx turbo build",
    "pkg-publish": "bun ./script/monorepo.ts pkg-publish",
    "typecheck": "tsgo --noEmit",
    "start-dev-svgminify": "cd pkg/svgminify && npm run dev",
    "start-dev-lost": "cd pkg/lost && npm run dev",
    "start-dev-ui": "cd pkg/ui && npm run dev",
    "clean-dist": "bun ./script/monorepo.ts clean-dist",
    "clean-node_modules": "bun ./script/monorepo.ts clean-nm",
    "clean-lock": "bun ./script/monorepo.ts clean-lock",
    "===test===": "",
    "test-command": "echo test",
    "test-list-sub-pkg": "bun ./script/monorepo.ts list-sub-pkg",
    "test-list-sub-pkg-package-json": "bun ./script/monorepo.ts list-sub-pkg-package-json",
    "test-file-replace-kv": "bun ./script/monorepo.ts file-replace-kv nk=version nv=abc"
  },
  "packageManager": "bun@1.2.13",
  "workspaces": [
    "pkg/*"
  ],
  "VERISON_EXT_PATH": [
    "./version/master.json",
    "./version/prod.json"
  ],
  "devDependencies": {
    "@rsbuild/core": "^1.4.13",
    "@rsbuild/plugin-node-polyfill": "^1.4.2",
    "@rsbuild/plugin-react": "^1.3.4",
    "@types/node": "^20.3.3",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript/native-preview": "^7.0.0-dev.20251204.1",
    "oxlint": "^1.22.0",
    "turbo": "^2.5.8",
    "typescript": "^5.1.6"
  }
}